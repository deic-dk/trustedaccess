<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>trustedaccess</id>
	<name>Trusted Access</name>
	<summary>Extensions for data processing integrations.</summary>
	<description><![CDATA[Webdav authorization by IP, by sharing, by x.509 certificate
Brute force protection
Hiding selected folders from webdav
File choosing utilities for remote clients.]]>
	</description>
	<version>0.0.1</version>
	<licence>agpl</licence>
	<author mail="frederik@orellana.dk" homepage="http://frederik.orellana.dk/">Frederik Orellana</author>
	<category>auth</category>
	<category>customization</category>
	<category>files</category>
	<category>integration</category>
	<category>tools</category>
	<bugs>https://github.com/deic-dk/trustedaccess/issues</bugs>
	<dependencies>
		<owncloud min-version="9.1" max-version="9.1"/>
		<nextcloud min-version="10" max-version="12"/>
	</dependencies>
	<remote>
		<mydav>appinfo/remote.php</mydav>
		<addcert>appinfo/addcert.php</addcert>
		<removecert>appinfo/removecert.php</removecert>
	</remote>
	<navigations>
